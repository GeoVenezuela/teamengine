<?xml version="1.0" encoding="UTF-8"?>	
<wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml" xmlns:ogc="http://www.opengis.net/ogc" version="1.1.0" service="WFS">
	<wfs:Query xmlns:sf="http://www.opengis.net/cite/gmlsf" typeName="sf:SimplePointFeature">
		<ogc:Filter>
			<ogc:PropertyIsEqualTo>
				<ogc:PropertyName>//gml:description</ogc:PropertyName>
				<ogc:Literal>description-g001</ogc:Literal>
			</ogc:PropertyIsEqualTo>
		</ogc:Filter>
	</wfs:Query>
</wfs:GetFeature>
